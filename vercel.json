{
  "version": 2,
  "builds": [
    {
      "src": "dist/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/survey",
      "dest": "/index.html"
    },
        {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    },
    {
      "src": "/survey/.*",
      "dest": "/index.html"
    },
    {
      "src": "/.*",
      "dest": "/index.html",
      "status": 200
    }
  ]
}